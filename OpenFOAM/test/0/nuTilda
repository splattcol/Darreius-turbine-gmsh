FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
	Inlet
	{
        type            fixedValue;
        value           $internalField;
	}
	outlet
	{
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
	}
	ami
	{ 
		type 			cyclicAMI;
		value 			$internalField; 
	}  
	ami1
	{ 
		type 			cyclicAMI;
		value 			$internalField; 
	} 
	blades
    {
        type            zeroGradient;
    }

    center
    {
        type            zeroGradient;
    }

    Front
    {
        type            empty;
    }
    Back
    {
        type            empty;
    }
    patch2
    {
        type            empty;
    }
    latWalls
    {
        type            empty;
    }
}

