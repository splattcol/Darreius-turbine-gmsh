FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.000765;

boundaryField
{
	Inlet
	{
        type            fixedValue;
        value           $internalField;
	}
	outlet
	{
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
	}
	ami
	{ 
		type 			cyclicAMI;
		value 			$internalField; 
	}
	ami1
	{ 
		type 			cyclicAMI;
		value 			$internalField; 
	}
    blades
    {
        type            epsilonWallFunction;
        value           uniform 0.000765;
    }
    center
    {
        type            epsilonWallFunction;
        value           uniform 0.000765;
    }
    Front
    {
        type            empty;
    }
    Back
    {
        type            empty;
    }
    patch2
    {
        type            empty;
    }
    latWalls
    {
        type            empty;
    }
}


