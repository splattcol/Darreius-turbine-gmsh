FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.14;

boundaryField
{
    FrontAndBack
    {
        type            empty;
    }
    patch1
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }
    patch2
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }
    patch3
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }
    Shaft
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }
    Inlet
    {
        type            freestream;
        freestreamValue uniform 0.14;
        value           uniform 0.14;
    }
    Lat-Wall
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }
    Outlet
    {
        type            freestream;
        freestreamValue uniform 0.14;
        value           uniform 0.14;
    }
    AMI-St
    {
        type            cyclicAMI;
        value           uniform 0;
    }
    AMI-Rt
    {
        type            cyclicAMI;
        value           uniform 0;
    }
}


