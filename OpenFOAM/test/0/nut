FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
Inlet
{
	type            fixedValue;
	value           $internalField;
}
Outlet
{
	type            inletOutlet;
	inletValue      $internalField;
	value           $internalField;
}
FrontAndBack
{
	type            empty;
}
Lat-Wall
{
       	type            empty;
}
AMI-St
{ 
	type 			cyclicAMI;
	value 			$internalField; 
}
AMI-Rt
{ 
	type 			cyclicAMI;
	value 			$internalField; 
}
Airfoil_1
{
	type            nutkWallFunction;
        value           uniform 0;
}
Airfoil_2
{
	type            nutkWallFunction;
	value           uniform 0;
}
Airfoil_3
{
	type            nutkWallFunction;
	value           uniform 0;
}
Shaft
{
	type            nutkWallFunction;
	value           uniform 0;
}
defaultFaces
{
	type            empty;
}
}

