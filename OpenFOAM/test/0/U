FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
Inlet
{
	type            fixedValue;
	value		uniform (5 0 0);
}
Outlet
{
	type            zeroGradient;
}
FrontAndBack
{
	type            empty;
}
Lat-Wall
{
       	type            empty;
}
AMI-St
{ 
	type 			cyclicAMI;
	value 			$internalField; 
}
AMI-Rt
{ 
	type 			cyclicAMI;
	value 			$internalField; 
}
Airfoil_1
{
	type            fixedValue;
        value           uniform (0 0 0);
}
Airfoil_2
{
	type            fixedValue;
        value           uniform (0 0 0);
}
Airfoil_3
{
	type            fixedValue;
        value           uniform (0 0 0);
}
Shaft
{
	type            fixedValue;
        value           uniform (0 0 0);
}
defaultFaces
{
	type            empty;
}
}

