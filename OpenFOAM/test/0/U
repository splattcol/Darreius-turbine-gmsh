FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
	Inlet
	{
		type			fixedValue;
		value			uniform (5 0 0);
	}
	outlet
	{
		type			zeroGradient;
	}
	ami
	{ 
		type 			cyclicAMI;
		value 			$internalField; 
	}
	ami1
	{ 
		type 			cyclicAMI;
		value 			$internalField; 
	}
    blades
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    center
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    Back
    {
        type            empty;
    }
    Front
    {
        type            empty;
    }
    patch2
    {
        type            empty;
    }
    latWalls
    {
        type            empty;
    }
}

