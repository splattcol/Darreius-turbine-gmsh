FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    FrontAndBack
    {
        type            empty;
    }
    patch1
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }
    patch2
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }
    patch3
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }
    Shaft
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }
    Inlet
    {
        type            fixedValue;
        value           uniform (2.8 0 0);
    }
    Lat-Wall
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }
    Outlet
    {
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value           uniform (0 0 0);
    }
    AMI-Rt
    {
        type            cyclicAMI;
        value           $internalField;
    }
    AMI-St
    {
        type            cyclicAMI;
        value           $internalField;
    }
}


